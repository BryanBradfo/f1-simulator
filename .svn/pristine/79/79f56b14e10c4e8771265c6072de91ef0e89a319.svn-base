package modele;

import java.io.Serializable;

public abstract class ElementVoiture implements Serializable {
	
	private static final long serialVersionUID = -8775640632517775156L;	
	
	/** Durée de vie de l'élément. */
	protected double dureeDeVie;
	
	/** Perte d'efficacité de l'élément. */
	public abstract void perteDeVie();
	
	public abstract ElementVoiture copie();
	
}
